n todo.id === id;\n      });\n      this.todos.splice(index, 1);\n    },\n    addTodo(e){\n      this.todos.push({\n        id: Math.random(),\n        text: e.target.value,\n        checked: false\n      });\n      this.todoText = '';\n    },\n    toggleCheckbox({id, checked}) {\n      const index = this.todos.findIndex(todo => {\n        return todo.id === id;\n      });\n      this.todos[index].checked = checked;\n    }\n  }\n}\n</script>"]}]}